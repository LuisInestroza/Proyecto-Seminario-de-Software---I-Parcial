    <div class="container 50%">
        <form action="/insumoUsuario" method="post">

            <div class="acciones btnVerde">
                <a href="">Presupuesto</a>
                <a href="/crearInsumo">Nuevo Insumo</a>
            </div>


            <p id="total">Total Actual:</p>



            <table class="table table-hover" id="insumo">

                <thead>
                    <tr>
                        <td scope="col">Descripción</td>
                        <td scope="col">Fecha</td>
                        <td scope="col">Total</td>
                        <td scope="col">Acciones</td>
                    </tr>
                </thead>
                {{#if insumos.length}}
                {{#each insumos}}
                <tbody class="insumo">
                    <tr>

                        <td>{{descripcion}}</td>
                        <td>{{fecha}}</td>
                        <td>L. {{total}}</td>
                        <td>
                            <div class="contenedor">
                                <a href="#" data-eliminar={{id}}>Eliminar</a>
                                <a href="/editarInsumo">Editar</a>
                            </div>

                        </td>
                    </tr>

                </tbody>
                {{/each}}
                {{/if}}
            </table>

        </form>
    </div>
</section>